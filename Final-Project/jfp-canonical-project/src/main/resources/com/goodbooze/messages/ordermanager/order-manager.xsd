<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:tp="http://goodbooze.com/types"
        targetNamespace="http://goodbooze.com/messages/ordermanager"
        elementFormDefault="qualified">

    <xs:import namespace="http://goodbooze.com/types" schemaLocation="../../types/db-entities.xsd"/>

    <xs:element name="ordermanager">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="supplierOrder" type="tp:supplierOrder" minOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>